class Solution {
public:
    int findMaxK(vector<int>& nums) {
        unordered_set<int> numSet(nums.begin(), nums.end());
        int ans= -1;
        for(auto it:numSet){
            if(numSet.find(-it)!=numSet.end()){
                ans= max(ans,it);
            }
        }
        return ans;
    }
};
